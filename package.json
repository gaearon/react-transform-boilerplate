{
  "name": "react-transform-boilerplate-vscode",
  "version": "1.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, error handling on module and component level and Visual Studio Code integration",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skolmer/react-transform-boilerplate-vscode.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Steffen Kolmer <steffen@kolmer.net> (http://github.com/skolmer)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/skolmer/react-transform-boilerplate-vscode/issues"
  },
  "homepage": "https://github.com/skolmer/react-transform-boilerplate-vscode",
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^1.0.7",
    "eslint": "^2.4.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-lodash": "^1.6.3",
    "eslint-plugin-react": "^4.3.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "ghooks": "^1.2.0",
    "html-webpack-plugin": "^2.10.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  }
}
